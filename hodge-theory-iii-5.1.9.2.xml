  <fr:mainmatter>
    <fr:tex display="block"><![CDATA[           \mathrm {d}(x^{pq})           = \mathrm {d}_K(x^{pq}) + (-1)^p\sum _i(-1)^i\delta _i x^{pq}.         \tag{5.1.9.2}         ]]></fr:tex>
  </fr:mainmatter>
  <fr:backmatter>
    <fr:tree toc="false" numbered="false" show-heading="true" show-metadata="false" expanded="true" root="false" xmlns:fr="http://www.jonmsterling.com/jms-005P.xml">
