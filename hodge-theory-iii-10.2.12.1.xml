    <fr:tex display="block"><![CDATA[         \overline {u}\colon  X\to  A         \quad \text {and}\quad          \overline {u}'\colon  X'\to  A'.       \tag{10.2.12.1}       ]]></fr:tex>
  </fr:mainmatter>
  <fr:backmatter>
    <fr:tree toc="false" numbered="false" show-heading="true" show-metadata="false" expanded="true" root="false" xmlns:fr="http://www.jonmsterling.com/jms-005P.xml">
      <fr:frontmatter>
