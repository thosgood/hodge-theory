  <fr:mainmatter>
    <fr:tex display="block"><![CDATA[         {}_{L_2}E_1^{pq}         = \operatorname {H}^q(\mathrm {s} K^{\bullet \bullet  p})         \Rightarrow  \operatorname {H}^{p+q}(\mathrm {s} K).       \tag{8.1.22.1}       ]]></fr:tex>
  </fr:mainmatter>
  <fr:backmatter>
    <fr:tree toc="false" numbered="false" show-heading="true" show-metadata="false" expanded="true" root="false" xmlns:fr="http://www.jonmsterling.com/jms-005P.xml">
      <fr:frontmatter>
        <fr:title text="Context">Context</fr:title>
