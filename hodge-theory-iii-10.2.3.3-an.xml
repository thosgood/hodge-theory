<fr:tree toc="true" numbered="true" show-heading="true" show-metadata="true" expanded="true" root="false" xmlns:fr="http://www.jonmsterling.com/jms-005P.xml">
  <fr:frontmatter>
    <fr:anchor>2023</fr:anchor>
    <fr:addr type="user">hodge-theory-iii-10.2.3.3-an</fr:addr>
    <fr:route>hodge-theory-iii-10.2.3.3-an.xml</fr:route>
